{
    "uniqueId": "template-project",
    "title": "Project",
    "description": "Project page with status, milestones, team, and documents",
    "isTemplate": true,
    "language": "en",
    "layout": {
        "columns": 1,
        "rows": [
            {
                "columns": 1,
                "backgroundColor": "var(--color-primary-element)",
                "widgets": [
                    {
                        "type": "heading",
                        "column": 1,
                        "order": 1,
                        "content": "Project Name",
                        "level": 1
                    },
                    {
                        "type": "text",
                        "column": 1,
                        "order": 2,
                        "content": "Brief description of the project, its goals, and expected outcomes. This page serves as the central hub for all project-related information."
                    }
                ]
            },
            {
                "columns": 2,
                "backgroundColor": "",
                "widgets": [
                    {
                        "type": "heading",
                        "column": 1,
                        "order": 1,
                        "content": "Project Status",
                        "level": 2
                    },
                    {
                        "type": "text",
                        "column": 1,
                        "order": 2,
                        "content": "**Status:** In Progress\n\n**Phase:** Development\n\n**Start Date:** January 15, 2025\n\n**Target Completion:** June 30, 2025\n\n**Budget Status:** On Track\n\n**Last Updated:** February 1, 2025"
                    },
                    {
                        "type": "heading",
                        "column": 2,
                        "order": 1,
                        "content": "Quick Links",
                        "level": 2
                    },
                    {
                        "type": "links",
                        "column": 2,
                        "order": 2,
                        "columns": 1,
                        "items": [
                            {
                                "title": "Project Plan",
                                "text": "View detailed timeline",
                                "url": "#",
                                "icon": "calendar-text",
                                "target": "_self"
                            },
                            {
                                "title": "Status Report",
                                "text": "Latest progress report",
                                "url": "#",
                                "icon": "file-chart",
                                "target": "_self"
                            },
                            {
                                "title": "Meeting Notes",
                                "text": "Recent meeting minutes",
                                "url": "#",
                                "icon": "note-multiple",
                                "target": "_self"
                            },
                            {
                                "title": "Risk Register",
                                "text": "Identified risks",
                                "url": "#",
                                "icon": "alert-circle",
                                "target": "_self"
                            }
                        ]
                    }
                ]
