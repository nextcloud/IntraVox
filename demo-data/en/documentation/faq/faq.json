                "backgroundColor": "var(--color-background-hover)",
                "widgets": [
                    {
                        "type": "heading",
                        "column": 1,
                        "order": 1,
                        "content": "What is IntraVox?",
                        "level": 3
                    },
                    {
                        "type": "text",
                        "column": 1,
                        "order": 2,
                        "content": "IntraVox is an open source intranet application for Nextcloud. It offers SharePoint-like functionality for creating internal websites, pages and content management - fully integrated with your Nextcloud environment."
                    }
                ]
            },
            {
                "columns": 1,
                "backgroundColor": "",
                "widgets": [
                    {
                        "type": "heading",
                        "column": 1,
                        "order": 1,
                        "content": "Is IntraVox free?",
                        "level": 3
                    },
                    {
                        "type": "text",
                        "column": 1,
                        "order": 2,
                        "content": "Yes! IntraVox is 100% open source and free to use under the AGPL license. There are no hidden costs or premium features. All functionality is available to everyone."
                    }
                ]
            },
            {
                "columns": 1,
                "backgroundColor": "var(--color-background-hover)",
                "widgets": [
                    {
                        "type": "heading",
                        "column": 1,
                        "order": 1,
                        "content": "Which Nextcloud version do I need?",
                        "level": 3
                    },
                    {
                        "type": "text",
                        "column": 1,
                        "order": 2,
                        "content": "IntraVox requires Nextcloud 27 or higher. We recommend always using the latest stable version of Nextcloud for the best experience and security updates."
                    }
                ]
            },
