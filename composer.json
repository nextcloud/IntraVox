{
  "name": "shalution/intravox",
  "description": "SharePoint-like pages for Nextcloud",
  "type": "nextcloud-app",
  "license": "AGPL-3.0",
  "require": {
    "php": ">=8.2",
    "enshrined/svg-sanitize": "^0.22"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "OCA\\IntraVox\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OCA\\IntraVox\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test:unit": "phpunit --testsuite Unit",
    "test:integration": "phpunit --testsuite Integration",
    "test:coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "audit": {
      "ignore": ["PKSA-4g5g-4rkv-myqs"]
    }
  }
}
